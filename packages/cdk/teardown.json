{
    "version": "0.2",
    "phases": {
      "install": {
        "runtime-versions": {
          "nodejs": 10
        }
      },
      "pre_build": {
        "commands": ["cd src/pipeline-template-cdk", "npm install"],
      },
      "build": {
        "commands": [
          "npm run build",

          "yes | npm run cdk destroy"
        ]
      }
    },
    "cache": {
      "paths": ["node_modules/**/*"]
  }
}
