{
    "version": "0.2",
    "phases": {
      "install": {
        "runtime-versions": {
          "nodejs": 12
        }
      },
      "pre_build": {
        "commands": ["cd src/pipeline-template-cdk", "npm uninstall -g aws-cdk" , "npm i -g aws-cdk@latest", "yarn install" ]
      },
      "build": {
        "commands": [
          "npm run build",
          "npm run cdk-confirmed"
        ]
      }
    },
    "cache": {
      "paths": ["node_modules/**/*"]
  }
}
